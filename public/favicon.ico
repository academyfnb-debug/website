<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a3e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f0f2a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00b8e6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="32" height="32" fill="url(#bgGradient)"/>
  
  <!-- FB Logo -->
  <g transform="translate(16,8)">
    <!-- F Letter -->
    <rect x="-8" y="-4" width="6" height="2" fill="url(#textGradient)"/>
    <rect x="-8" y="-4" width="2" height="12" fill="url(#textGradient)"/>
    <rect x="-8" y="2" width="4" height="2" fill="url(#textGradient)"/>
    
    <!-- B Letter -->
    <rect x="2" y="-4" width="2" height="12" fill="url(#textGradient)"/>
    <rect x="2" y="-4" width="4" height="2" fill="url(#textGradient)"/>
    <rect x="2" y="2" width="3" height="2" fill="url(#textGradient)"/>
    <rect x="2" y="6" width="4" height="2" fill="url(#textGradient)"/>
    <rect x="5" y="-2" width="2" height="3" fill="url(#textGradient)"/>
    <rect x="5" y="4" width="2" height="2" fill="url(#textGradient)"/>
  </g>
  
  <!-- Academy Text -->
  <text x="16" y="28" font-family="Arial, sans-serif" font-size="6" font-weight="bold" text-anchor="middle" fill="url(#textGradient)">ACADEMY</text>
</svg>